/* 容器样式 */
.container{
  box-sizing: border-box;
}
/* 文本公共样式 */
text{
  display: block;
}
/* 头像 */
#headImg{
  border-radius: 50%;
  overflow: hidden;
  width: 250rpx;
  height: 250rpx;
  margin: 10rpx auto;
}
/*姓名学号*/
.name-num text{
  text-align: center;
  font-size: 50rpx;
  font-family: '宋体';
}
/*学校学院积分*/
.secondcontainer {
  margin: 20rpx 0 10rpx 25rpx;
}
.secondcontainer text{
  color: #555;
}
/*修改信息按钮*/
.changeInfo{
  width: 700rpx;
  margin: 0 25rpx;
}